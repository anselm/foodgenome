<h1>Please enter your email and we will send a new password</h1>

<%= error_messages_for :user %>

<% form_for(:user, :url => "/forgot", :html => { :multipart => true } ) do |f| -%>
<p><label for="email">Email</label><br/>
<%= f.text_field :email %></p>

<p><%= submit_tag 'Recover' %></p>
<% end -%>

